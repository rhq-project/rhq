               <entry key="rhq.agent.plugins.event-sender.initial-delay-secs" value="@@@rhq.agent.plugins.event-sender.initial-delay-secs@@@"/>
               <entry key="rhq.agent.plugins.event-sender.period-secs" value="@@@rhq.agent.plugins.event-sender.period-secs@@@"/>
               <entry key="rhq.agent.plugins.event-report.max-per-source" value="@@@rhq.agent.plugins.event-report.max-per-source@@@"/>
               <entry key="rhq.agent.plugins.event-report.max-total" value="@@@rhq.agent.plugins.event-report.max-total@@@"/>
