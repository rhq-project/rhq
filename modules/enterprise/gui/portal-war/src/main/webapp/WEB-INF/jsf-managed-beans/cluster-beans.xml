<?xml version="1.0"?>

<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

   <!-- /rhq/ha/listServers.xhtml -->
   <managed-bean>
      <managed-bean-name>ListServersUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.ha.ListServersUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>

   <!-- /rhq/ha/listAgents.xhtml -->
   <managed-bean>
      <managed-bean-name>ListAgentsUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.ha.ListAgentsUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>

   <!-- /rhq/ha/viewServer.xhtml -->
   <managed-bean>
      <managed-bean-name>ViewServerUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.ha.ViewServerUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>
   
   <!-- /rhq/ha/viewAffinityGroup.xhtml -->
   <managed-bean>
      <managed-bean-name>ViewAffinityGroupDetailsUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.ha.ViewAffinityGroupDetailsUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>
   
   <managed-bean>
      <managed-bean-name>ViewAffinityGroupAgentMembersUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.ha.ViewAffinityGroupAgentMembersUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>
   
   <managed-bean>
      <managed-bean-name>ViewAffinityGroupServerMembersUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.ha.ViewAffinityGroupServerMembersUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>

</faces-config>