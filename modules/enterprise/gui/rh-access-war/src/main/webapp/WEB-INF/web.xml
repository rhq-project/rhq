<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<display-name>rhaccess</display-name>

	<welcome-file-list>
		<welcome-file>support.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>attachments</servlet-name>
		<servlet-class>org.rhq.server.rhaccess.Attachments</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>attachments</servlet-name>
		<url-pattern>/attachments</url-pattern>
	</servlet-mapping>
</web-app>