<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    version="2.5">

    <display-name>rhaccess</display-name>

    <welcome-file-list>
        <welcome-file>support-redhat-1.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>attachments</servlet-name>
        <servlet-class>org.rhq.server.rhaccess.AttachmentsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>attachments</servlet-name>
        <url-pattern>/attachments</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>config</servlet-name>
        <servlet-class>org.rhq.server.rhaccess.ConfigServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>config</servlet-name>
        <url-pattern>/configuration</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>CacheControlFilter</filter-name>
        <filter-class>org.rhq.server.rhaccess.CacheControlFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CacheControlFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>